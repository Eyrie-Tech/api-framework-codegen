{
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Debug codegen tool",
      "type": "node",
      "program": "${workspaceFolder}/main.ts",
      "cwd": "${workspaceFolder}",
      "env": {},
      "runtimeExecutable": "/opt/homebrew/bin/deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-wait",
        "--allow-all"
      ],
      "args": [
        "--spec",
        "../eyrie.yaml"
      ],
      "attachSimplePort": 9229
    }
  ]
}
